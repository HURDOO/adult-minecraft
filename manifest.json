{
    "name": "Adult Minecraft",
    "description": "마인크래프트는 성인용 게임입니다. minecraft.net은 청소년 유해 사이트입니다.",
    "version": "1.0",
    "manifest_version": 2,
    
    "background" : {
        "scripts": ["background.js"]
    },
    "permissions": [
        "webNavigation",
        "*://*/*",
        "http://*/",
        "https://*/"
    ],
    "icons": {
        "16": "./16.png",
        "32": "./32.png",
        "48": "./48.png",
        "128": "./128.png"
      }
  }